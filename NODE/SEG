var superagent = require('superagent');
var fs = require("fs");
superagent.post('https://segmentfault.com/api/user/login?_=8b213f910292c9c944b623851248a6b5')
    .set('Referer', 'https://segmentfault.com/u/dazhuaoyao_582286fea6622')
    .set('X-Requested-With', 'XMLHttpRequest')
    .set('Cookie', 'PHPSESSID=web2~j78d7m5anmjudqp243aa9lq8o3; _gat=1; _ga=GA1.2.350653874.1482997010; Hm_lvt_e23800c454aa573c0ccb16b52665ac26=1482997010,1483409687,1483501836; Hm_lpvt_e23800c454aa573c0ccb16b52665ac26=1483505877')
    .send({
        username: "填写登录时的qq邮箱",
        password: "填写登陆时密码"
    })
    .type('form')
    .end(function(err, res) {
        if (err || !res.ok) {
            console.log(err.status);
        } else {
			fs.writeFile('input.txt', JSON.stringify(res),  function(err) {
				
			});
   console.log('yay got ' + JSON.stringify(res.body));
        }
    });
    
    优秀参考案例（排名不分先后）：
    http://www.tuicool.com/articles/VVNR32n
    https://segmentfault.com/a/1190000003851057#articleHeader1
    http://cnodejs.org/topic/56908ea6c301558265041ded
    https://github.com/starkwang/Zhihu-Spider
    
    
    
